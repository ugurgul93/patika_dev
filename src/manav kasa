import java.util.Scanner;

public class manav_kasa_programi {
    public static void main(String[] args) {

        int Armut, Elma, Domates, Muz, Patlican;

        double fiyatArmut = 2.14, fiyatElma = 3.67, fiyatDomates = 1.11, fiyatMuz = 0.95, fiyatPatlican = 5.00;


        Scanner inp = new Scanner(System.in);

        System.out.print("Armut Kac Kilo ? : ");
        Armut = inp.nextInt();

        System.out.print("Elma Kac Kilo ? : ");
        Elma =  inp.nextInt();

        System.out.print("Domates Kac Kilo ? : ");
        Domates = inp.nextInt();

        System.out.print("Muz Kac Kilo ? : ");
        Muz = inp.nextInt();

        System.out.print("Patlican Kac Kilo ? : ");
        Patlican = inp.nextInt();

        double tutar = (Armut * fiyatArmut) + (Elma * fiyatElma) + (Domates * fiyatDomates) + (Muz * fiyatMuz) + (Patlican * fiyatPatlican);


        System.out.println("Toplam Tutar : " + tutar);




    }
}
